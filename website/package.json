    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "classnames": "^2.2.6",
    "meteor-node-stubs": "^0.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "5.3",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.2",
